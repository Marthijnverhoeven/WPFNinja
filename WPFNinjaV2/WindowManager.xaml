<Window x:Class="WPFNinjaV2.WindowManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowManager" Height="132" Width="300"
        DataContext="{Binding Manager, Source={StaticResource Locator}}">

    <StackPanel Margin="0,0,0,0">
        <Button Command="{Binding ShowMainWindowCommand}" Height="50" Content="Items Admin"/>
        <Button Command="{Binding ShowSecondWindowCommand}" Height="50" Content="Ninja Manager"/>
    </StackPanel>
</Window>
